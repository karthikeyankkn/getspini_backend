<table>                         
  <tr>                          
    <th>title</th>
    <th>Offer Details</th>
    <th>Address</th>
    <th>Description</th>
    <th>About</th>
    <th>Terms</th>
    <th>Seo_Url</th>
    <th>offer_img_url</th>
    <th>banner_url</th>
    <th>Action</th>
  </tr>
<% @property_deal.each do |data| %>       
  <tr>
    <td><%= data.title %></td> 
    <td><%= data.offer_details %></td>  
    <td><%= data.address %></td>  
    <td><%= data.description %></td>  
    <td><%= data.about %></td>  
    <td><%= data.terms %></td>  
    <td><%= data.seo_url %></td>  
    <td><%= data.offer_img_url %></td>  
    <td><%= data.banner_url %></td> 
    <td><%= link_to "Delete", property_deal_path(data.id), method: :delete, data:{confirm: "Are you sure you want to delete"} %></td>
  </tr>
<% end %>                        
</table>    