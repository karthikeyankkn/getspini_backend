<!DOCTYPE html>
<html>
  <head>
    <title>GetspiniBackend</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script> -->
  </head>
  <style type="text/css">
    body{
      background: #f0f0f2;
    }
    .well{
      background: #fff;
    }
    img{
      width: 100%;
    }
    .margin-tb{
      margin: 10px 0;
    }
    .pagination a{
      background: #F44336;
      color: #fff;
      padding: 8px;
    }
    .current{
      background: #00BCD4;
      color: #fff;
      padding: 8px;
    }
  </style>
  <body>
    <header>
      <nav class="navbar navbar-inverse">
        <div class="container-fluid">
          <div class="navbar-header">
            <a class="navbar-brand" href="/property_deals_showall">GETSPINI</a>
          </div>
          <ul class="nav navbar-nav">
            <li class="active"><a href="/property_deals_showall">Home</a></li>
            <li><a href="/property_deals/new">Create new offer</a></li>
            <li><a href="/property_deals_showall">View all offers</a></li>
            <li><a href="/leads">Show Leads</a></li>
            <li><a href="/leads/new">Create Leads</a></li>
          </ul>
        </div>
      </nav>
    </header>
    <%= yield %>
  </body>
</html>
